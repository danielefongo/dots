# vim:ft=dosini

[delta]
  syntax-theme                  = "ansi"
  commit-style                  = raw
  file-style                    = "#6cbeff"
  hunk-header-style             = syntax
  minus-style                   = normal "#e03d63"
  minus-non-emph-style          = normal "#e03d63"
  minus-emph-style              = reverse "#ff3765"
  minus-empty-line-marker-style = normal "#e03d63"
  zero-style                    = syntax
  plus-style                    = normal "#93bf50"
  plus-non-emph-style           = normal "#93bf50"
  plus-emph-style               = reverse "#aaff29"
  plus-empty-line-marker-style  = normal "#93bf50"
  grep-file-style               = "#6cbeff"
  grep-line-number-style        = "#6cbeff"
  whitespace-error-style        = reverse "#c163ff"
  blame-palette                 = #191d27 #474b56 #6f737c
